
(rules PCB bytecradle-6502-single-board
  (snap_angle 
    fortyfive_degree
  )
  (autoroute_settings
    (fanout off)
    (autoroute on)
    (postroute on)
    (vias on)
    (via_costs 50)
    (plane_via_costs 5)
    (start_ripup_costs 100)
    (start_pass_no 13693)
    (layer_rule F.Cu
      (active on)
      (preferred_direction horizontal)
      (preferred_direction_trace_costs 1.8)
      (against_preferred_direction_trace_costs 3.1)
    )
    (layer_rule In1.Cu
      (active on)
      (preferred_direction vertical)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 1.8)
    )
    (layer_rule In2.Cu
      (active on)
      (preferred_direction horizontal)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 2.3)
    )
    (layer_rule B.Cu
      (active on)
      (preferred_direction vertical)
      (preferred_direction_trace_costs 1.8)
      (against_preferred_direction_trace_costs 2.6)
    )
  )
  (rule
    (width 200.0)
    (clear 200.0)
    (clear 100.0 (type smd_to_turn_gap))
    (clear 50.0 (type smd_smd))
  )
  (padstack "Via[0-3]_600:300_um"
    (shape
      (circle F.Cu 600.0 0.0 0.0)
    )
    (shape
      (circle In1.Cu 600.0 0.0 0.0)
    )
    (shape
      (circle In2.Cu 600.0 0.0 0.0)
    )
    (shape
      (circle B.Cu 600.0 0.0 0.0)
    )
    (attach off)
  )
  (via 
    "Via[0-3]_600:300_um" "Via[0-3]_600:300_um" default
  )
  (via 
    "Via[0-3]_600:300_um-kicad_default" "Via[0-3]_600:300_um" "kicad_default"
  )
  (via_rule
    default "Via[0-3]_600:300_um"
  )
  (via_rule
    "kicad_default" "Via[0-3]_600:300_um-kicad_default"
  )
  (class default
    (clearance_class default)
    (via_rule default)
    (rule
      (width 200.0)
    )
    (circuit 
      (use_layer F.Cu In1.Cu In2.Cu B.Cu)
    )
  )
  (class "kicad_default"
    "Net-(U5-XTAL1)" GND VCC "Net-(D1-K)" "Net-(J2-Shield)" "unconnected-(J2-D--Pad2)" "unconnected-(J2-D+-Pad3)" ~{RTS}
    RxD TxD ~{CTS} "Net-(U1-RDY)" ~{IRQ} "Net-(U5-XTAL2)" ~{RES} ~{NMI}
    "Net-(RN1-R3)" "unconnected-(RN1-R7-Pad8)" "unconnected-(RN1-R6-Pad7)" "Net-(RN1-R4)" "unconnected-(RN1-R5-Pad6)" "Net-(RN1-R1)" "Net-(RN1-R2)" "unconnected-(RN1-R8-Pad9)"
    D2 D1 A5 A12 D7 A15 A0 "unconnected-(U1-~{SO}-Pad38)"
    D6 A2 "unconnected-(U1-ϕ2-Pad39)" A4 A13 A1 A11 CLK
    "unconnected-(U1-nc-Pad35)" D0 A8 "unconnected-(U1-SYNC-Pad7)" "unconnected-(U1-~{ML}-Pad5)" A14 "unconnected-(U1-~{VP}-Pad1)" D3
    "unconnected-(U1-ϕ1-Pad3)" A7 D5 A6 A10 A3 R{slash}~{W} D4
    A9 "Net-(U2-CE)" BANK2 BANK0 BANK4 BANK1 BANK3 RAMBANK
    REG6 REG7 "Net-(U3-Cp)" ~{I{slash}O} "Net-(U12-P=R)" "unconnected-(U5-~{DTR}-Pad11)" "unconnected-(U5-RxC-Pad5)" "Net-(U8-~{CS})"
    "Net-(U9-CE)" "Net-(U10-P=R)" CA1 PB4 CA2 PB0 PA4 PA5
    PB6 CB2 PA7 PA6 CB1 PA2 PB5 PB7
    PB3 PB1 "Net-(U13-~{CS2})" PA0 PA3 PA1 PB2 "unconnected-(U14-QB-Pad2)"
    "Net-(U14-~{BO})" "Net-(U14-DOWN)" "unconnected-(U14-QD-Pad7)" "unconnected-(U14-~{CO}-Pad12)" "unconnected-(U14-QC-Pad6)" "unconnected-(U14-QA-Pad3)" "unconnected-(U15-~{CO}-Pad12)" "unconnected-(U15-QC-Pad6)"
    "unconnected-(U15-QB-Pad2)" "Net-(U15-QA)" "unconnected-(U15-~{BO}-Pad13)" "unconnected-(U15-B-Pad1)" "unconnected-(U15-D-Pad9)" "unconnected-(U15-A-Pad15)" "unconnected-(U15-QD-Pad7)" "unconnected-(U15-C-Pad10)"
    "unconnected-(U16-Pad8)" "unconnected-(U16-Pad10)" "Net-(U16-Pad2)" "unconnected-(U16-Pad12)" "unconnected-(X1-EN-Pad1)" "Net-(U7-~{CS})" "unconnected-(U6-Pad6)" "Net-(U8-~{OE})"
    "Net-(U11-Pad2)" "Net-(U11-Pad13)" "Net-(U11-Pad10)" "Net-(U18-Pad12)" "Net-(U11-Pad5)" ~{CLK} ~{IRQC51} ~{IRQC22}
    "unconnected-(U19-Pad8)" "unconnected-(U19-Pad6)" "unconnected-(U19-Pad11)"
    (clearance_class "kicad_default")
    (via_rule kicad_default)
    (rule
      (width 200.0)
    )
    (circuit 
      (use_layer F.Cu In1.Cu In2.Cu B.Cu)
    )
  )
)